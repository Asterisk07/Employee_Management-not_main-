
def self_add():
    top=Toplevel()
    #top.deiconify()
    frame=Frame(top)
    frame.pack()
    a=Label(frame,text="Enter Name")
    a.grid(row=0,column=0)
    b=Label(frame,text="Enter email")
    b.grid(row=1,column=0)
    name=StringVar()
    email=StringVar()
    e1=Entry(frame,textvariable=name)
    e1.grid(row=0,column=1)
    e2=Entry(frame,textvariable=email)
    e2.grid(row=1,column=1)
    
    

